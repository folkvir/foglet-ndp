{
  "name": "foglet-ndp",
  "version": "0.1.0",
  "description": "This project aims to provide a simple neighbours delegated protocol in order to reduce query execution time",
  "main": "foglet-ndp.js",
  "scripts": {
    "doc": "jsdoc -c .jsdoc.json README.md src/",
    "test": "npm run karma",
    "karma": "karma start karma.config.js",
    "cover": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build:ndp": "browserify -r ./foglet-ndp.js:foglet-ndp -t envify -t babelify --presets es2015   > ./build/foglet-ndp.bundle.js",
    "build:watch": "watchify -r ./foglet-ndp.js:foglet-ndp -t envify -t babelify --presets es2015  -o ./build/foglet-ndp.bundle.js -v -d "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/folkvir/foglet-ndp.git"
  },
  "keywords": [
    "spray-wrtc",
    "foglet-core",
    "neighbours",
    "delegated",
    "protocol"
  ],
  "author": "Grall Arnaud (Folkvir)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/folkvir/foglet-ndp/issues"
  },
  "homepage": "https://github.com/folkvir/foglet-ndp#readme",
  "dependencies": {
    "events": "^1.1.1",
    "foglet-core": "^0.1.1",
    "immutable": "^3.8.1",
    "ldf-client": "^2.0.4",
    "q": "^1.4.1",
    "unicast-definition": "^0.1.0",
    "version-vector-with-exceptions": "0.0.5"
  },
  "peerDependencies": {
    "spray-wrtc": "^0.1.x"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-istanbul": "^2.0.0",
    "chai": "^3.5.0",
    "envify": "^4.0.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "jquery": "^3.1.1",
    "jsdoc": "^3.4.3",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-edge-launcher": "^0.1.1",
    "karma-express-http-server": "0.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-socketio-server": "^1.0.0",
    "minifyjs": "^0.2.0",
    "mocha": "^3.1.2",
    "nyc": "^8.3.2",
    "spray-wrtc": "^0.1.1",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
}
